<?xml version="1.0" encoding="utf-8"?>
<setup>

    <import platforms="db"/>
    <database id="database" environment="database"/>
    <execute type="sql" target="database">
        drop table if exists sample;
        create table sample (
        id int primary key
        );
    </execute>
    <generate type="sample" count="1" consumer="database"/>
</setup>